{
  "name": "patapura-app",
  "version": "1.0.9",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "prebuild": "npx expo prebuild",
    "build:development:ios": "npx expo prebuild && eas build --profile development --platform ios",
    "build:development:android": "npx expo prebuild && eas build --profile development --platform android",
    "build:staging": "npx expo prebuild && eas build --platform all --profile preview",
    "build:staging:ios": "npx expo prebuild && eas build --platform ios --profile preview",
    "build:staging:android": "npx expo prebuild && eas build --platform android --profile preview",
    "build:production": "npx expo prebuild && eas build --platform all --profile production",
    "build:production:android": "npx expo prebuild && eas build --platform android --profile production",
    "submit:ios": "eas submit -p ios",
    "submit:android": "eas submit -p android",
    "publish:staging": "eas update --branch staging",
    "publish:production": "eas update --branch production",
    "lint": "npx tsc --noEmit && npx eslint",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo/config-plugins": "^6.0.1",
